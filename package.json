{
  "name": "@rugo-vn/vue",
  "version": "1.2.0-beta.1",
  "main": "./dist/rugo-vue.umd.js",
  "module": "./dist/rugo-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/rugo-vue.es.js",
      "require": "./dist/rugo-vue.umd.js"
    },
    "./theme.js": "./src/theme.js"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "make-pack": "mkdir -p build && npm pack && mv rugo-vn-vue-* build/rugo-vn-vue.tgz",
    "publish:beta": "npm run build && npm publish --access public --tag beta",
    "publish:release": "npm run build && npm publish --access public"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "^7.0.18",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/vue3": "^7.0.18",
    "@storybook/vue3-vite": "^7.0.18",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-vue": "^9.11.0",
    "pascalcase": "^2.0.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "storybook": "^7.0.18",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.9",
    "vite-svg-loader": "^4.0.0"
  },
  "dependencies": {
    "ramda": "^0.29.0",
    "vee-validate": "^4.9.6"
  },
  "peerDependencies": {
    "@heroicons/vue": "^2.0.18",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  }
}
